.ui-widget {
    font-size: 12px !important;
}
.ui-layout-north {
    z-index: 20 !important;
    overflow: visible !important;;
}
.ui-layout-north .ui-layout-unit-content {
    overflow: visible !important;
}
.ui-widget-header {
    text-align: center;
    text-transform: uppercase;
}
.ui-button {
    margin-top: 10px !important;
}
.ui-layout-resizer {
    background-color: dimgrey !important;
    padding: 4px !important;
}

.ui-slidemenu-wrapper, .ui-slidemenu-content {
    height: 145px !important;
}

.ui-panelgrid.ui-panelgrid-blank .ui-panelgrid-content.ui-widget-content, 
.ui-panelgrid.ui-panelgrid-blank .ui-panelgrid-header.ui-widget-header,
.ui-panelgrid.ui-panelgrid-blank .ui-panelgrid-footer.ui-widget-header,
.ui-panelgrid.ui-panelgrid-blank .ui-panelgrid-header > tr.ui-widget-header, 
.ui-panelgrid.ui-panelgrid-blank tbody > tr.ui-widget-content,
.ui-panelgrid.ui-panelgrid-blank tbody > tr.ui-widget-content > td.ui-panelgrid-cell,
.ui-panelgrid.ui-panelgrid-blank .ui-panelgrid-footer > tr.ui-widget-footer {
    background: none !important;
    border: 0 none !important;
}

.ui-panel {
    background: none !important;
    border: 0 none !important;
    border-style: hidden !important;
}

.create-device {
    text-align: center !important;
    text-align:  -moz-center !important;
    background: none !important;
    border: 0 none !important;
    border-style: hidden !important;
    width: 40% !important;
}

.pending-status {
    color: mediumturquoise !important;
    font-size: 1.3em;
    -webkit-background-clip: text;
    -moz-background-clip: content-box;
    background-clip: border-box;
    color: transparent;
    text-shadow: rgba(255,255,255,0.3) 0px 3px 3px;

}

.pending-panel-grid{
    color: rgba(0,0,0,0.6);
    display: block; 
    margin: 10px 0px; 
    font-size: 1.2em;
    text-shadow: 0 1px 5px rgba(255, 255, 255, 0.3);
    text-transform: uppercase;
}

.pending-assign-button {
    font-size: 93% !important ;
    text-transform: capitalize; 
    font-family: Comic Sans MS; 
    color: #1cb3ef !important;
}

.create-invoice-price { 
    color: rgb(232, 240, 243) !important;
    /*    text-shadow: 2px 8px 6px rgba(0,0,0,0.2),
            0px -5px 35px rgba(255,255,255,0.3) !important;*/
    text-shadow: 1px 1px 3px #0099E0 !important;
    float: right !important;
}

#OfficeOrderViewForm .ui-column-filter {

    width: inherit;
    width: -moz-available !important;
}

.custom-panel-grp {
    text-align: center !important;
}

#InvoiceCreateForm .ui-selectonemenu  {
    min-width: 132px !important;
    width: 132px !important;
}
#OfficeOrderCreateForm .ui-selectonemenu , .ui-inputtextarea  {
    min-width: 132px !important;
    width: 132px !important;
    width: available ;
}
#OfficeOrderCreateForm .ui-inputtextarea  {
    min-width: -moz-available  !important;
    width: available !important;
}

