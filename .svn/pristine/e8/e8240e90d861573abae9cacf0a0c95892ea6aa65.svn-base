<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="PartViewDlg" widgetVar="PartViewDialog" modal="true" resizable="false" appendTo="@(body)" header="#{msgs_EN.ViewPartTitle}">
            <h:form id="PartViewForm">
                <h:panelGroup id="display">
                    <p:panelGrid columns="2" rendered="#{partController.selected != null}">
                        <h:outputText value="#{msgs_EN.ViewPartLabel_partId}"/>
                        <h:outputText value="#{partController.selected.partId}" title="#{msgs_EN.ViewPartTitle_partId}"/>
                        <h:outputText value="#{msgs_EN.ViewPartLabel_partInfo}"/>
                        <h:outputText value="#{partController.selected.partInfo}" title="#{msgs_EN.ViewPartTitle_partInfo}"/>
                        <h:outputText value="#{msgs_EN.ViewPartLabel_partUnitPrice}"/>
                        <h:outputText value="#{partController.selected.partUnitPrice}" title="#{msgs_EN.ViewPartTitle_partUnitPrice}"/>
                        <h:outputText value="#{msgs_EN.ViewPartLabel_partInstock}"/>
                        <h:outputText value="#{partController.selected.partInstock}" title="#{msgs_EN.ViewPartTitle_partInstock}"/>
                    </p:panelGrid>
                    <p:commandButton value="#{msgs_EN.Close}" onclick="PartViewDialog.hide()"/>
                </h:panelGroup>
            </h:form>
        </p:dialog>

    </ui:composition>
</html>
