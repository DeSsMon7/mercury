<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="ServicesListViewDlg" widgetVar="ServicesListViewDialog" modal="true" resizable="false" appendTo="@(body)" header="#{msgs_EN.ViewServicesListTitle}">
            <h:form id="ServicesListViewForm">
                <h:panelGroup id="display">
                    <p:panelGrid columns="2" rendered="#{servicesListController.selected != null}">
                        <h:outputText value="#{msgs_EN.ViewServicesListLabel_serviceListId}"/>
                        <h:outputText value="#{servicesListController.selected.serviceListId}" title="#{msgs_EN.ViewServicesListTitle_serviceListId}"/>
                        <h:outputText value="#{msgs_EN.ViewServicesListLabel_serviceId}"/>
                        <h:outputText value="#{servicesListController.selected.serviceId}" title="#{msgs_EN.ViewServicesListTitle_serviceId}"/>
                        <h:outputText value="#{msgs_EN.ViewServicesListLabel_serviceOrderId}"/>
                        <h:outputText value="#{servicesListController.selected.serviceOrderId}" title="#{msgs_EN.ViewServicesListTitle_serviceOrderId}"/>
                    </p:panelGrid>
                    <p:commandButton value="#{msgs_EN.Close}" onclick="ServicesListViewDialog.hide()"/>
                </h:panelGroup>
            </h:form>
        </p:dialog>

    </ui:composition>
</html>
